<% include partials/head.ejs %>
<title><%= data.name %> - Dating app</title>

<!--navbar-->
<% include partials/header.ejs %>

<!--Body-->
<main class="profile-main">
    <section>
        <% if (data.profile) { %>
            <img alt="<%= data.name %>" class="profile-picture" src="/upload/<%= data.profile %>">
        <% } %>
        <h1><%= data.name %></h1>
        <p><%= data.age %></p>
        <button id=js-remove data-id="<%= data.id %>" class=danger>Remove</button>
        <p class="js-delete">Could not delete!</p>
    </section>
</main>

<!--Footer-->
<% include partials/footer.ejs %>

<!--Script tags-->
<% include partials/tail.ejs %>